@using System.Web.UI.HtmlControls
@using ToleenProjekt.Controllers


<!DOCTYPE html>

<html>
<head>
    <meta name="viewport" content="width=device-width" />
    <title>_Layout</title>
    <style>
        ul {
                list-style-type: none;
                margin: 0;
                padding: 0;
                overflow: hidden;
                background-color: #333333;
        }

        li {
                float: left;
        }

            li a {
                    display: block;
                    color: white;
                    text-align: center;
                    padding: 16px;
                    text-decoration: none;
            }

                li a:hover {
                        background-color: #111111;
                }
    </style>

</head>
<body>
    <ul>
        <li>@Html.ActionLink("Home","Home","Home")</li>
        <li>@Html.ActionLink("Go", "Go", "Home")</li>
        <li>@Html.ActionLink("Come", "Come", "Home")</li>    
    </ul>
@RenderBody()
</body>
</html>
